{
  "name": "Template",
  "version": "0.0.1",
  "dependencies": {
    "babelify": "^7.3.0",
    "gulp-babel": "^7.0.0",
    "gulp-browserify": "^0.5.1",
    "gulp-rollup": "^2.15.0",
    "gulp-sourcemaps": "^2.4.1",
    "jquery": "~3.1.1",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2"
  },
  "devDependencies": {
    "autoprefixer": "~6.7.5",
    "babel-core": "~6.23.1",
    "babel-eslint": "~7.1.1",
